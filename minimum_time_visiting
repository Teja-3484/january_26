class Solution:
    def minTimeToVisitAllPoints(self, points: List[List[int]]) -> int:
        time = 0
        for i in range(len(points)-1):
            for j in range(1):
                x = abs(points[i+1][j] - points[i][j])
                y = abs(points[i+1][j+1] - points[i][j+1])
                if x > y:
                    time = time + x
                else:
                    time = time + y
        return time
